{% extends 'base.html.twig' %}

{% block title %}Piece-Jointe{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Piece-Jointe</h1>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ piece_jointe.id }}</td>
            </tr>
            <tr>
                <th>NomFichier</th>
                <td>{{ piece_jointe.nomFichier }}</td>
            </tr>
            <tr>
                <th>NomServer</th>
                <td>{{ piece_jointe.nomServer }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ piece_jointe.reference }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('piece_jointe_index') }}">Retour</a>

    <a href="{{ path('piece_jointe_edit', {'id': piece_jointe.id}) }}">Edition</a>

    {{ include('piece_jointe/_delete_form.html.twig') }}
</div>
{% endblock %}
