{% extends 'base.html.twig' %}

{% block title %}Mouvement{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Mouvement</h1>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mouvement.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ mouvement.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('mouvements_index') }}">Retour</a>

    <a href="{{ path('mouvements_edit', {'id': mouvement.id}) }}">Edition</a>

    {{ include('mouvements/_delete_form.html.twig') }}
</div>
{% endblock %}
