{% extends 'base.html.twig' %}

{% block title %}Etats{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Etats</h1>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etat.id }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ etat.etat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('etats_index') }}">Retour</a>

    <a href="{{ path('etats_edit', {'id': etat.id}) }}">Edition</a>

    {{ include('etats/_delete_form.html.twig') }}
</div>
{% endblock %}
