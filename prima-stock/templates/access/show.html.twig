{% extends 'base.html.twig' %}

{% block title %}Access{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Access</h1>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ access.id }}</td>
            </tr>
            <tr>
                <th>Access</th>
                <td>{{ access.access }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('access_index') }}">Retour</a>

    <a href="{{ path('access_edit', {'id': access.id}) }}">Edition</a>

    {{ include('access/_delete_form.html.twig') }}
</div>
{% endblock %}
